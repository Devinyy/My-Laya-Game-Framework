this.Road888.active = true;\r\n    }\r\n    setUnitByData() {\r\n        const dataArr = GameData.Level.getUnitDataArr();\r\n        for (let index = 0; index < dataArr.length; index++) {\r\n            let unit;\r\n            const unitObj = dataArr[index];\r\n            if (GameRes.Prefab3D[unitObj.name] && GameRes.Prefab3D[unitObj.name].instance) {\r\n                unit = GameRes.Prefab3D[unitObj.name].instance.clone();\r\n            }\r\n            else {\r\n                console.log('路段', unitObj, '不存在！');\r\n                continue;\r\n            }\r\n            unit[this.proUnitLen] = 60;\r\n            unit[this.proIndex] = index;\r\n            this.unitParent.addChild(unit);\r\n            unit.transform.position = new Laya.Vector3(0, 0, this.lenTotal);\r\n            if (unitObj.name !== 'unit_999') {\r\n                this.lenTotal += unitObj.length;\r\n            }\r\n            else {\r\n                this.lenTotal += 5;\r\n            }\r\n        }\r\n    }\r\n    unitDisplay(numShow = 2, numHide = 0.5) {\r\n        Laya.timer.frameLoop(1, this, () => {\r\n            if (this.Road888.transform.position.z + 10 < this.camaraPoint.transform.localPositionZ) {\r\n                if (this.Road888.active) {\r\n                    this.Road888.active = false;\r\n                }\r\n            }\r\n            for (let index = 0; index < this.unitParent.numChildren; index++) {\r\n                const element = this.unitParent.getChildAt(index);\r\n                if (element.transform.position.z < this.camaraPoint.transform.localPositionZ) {\r\n                    if (this.camaraPoint.transform.localPositionZ - element.transform.position.z > numHide * element[this.proUnitLen]) {\r\n                        if (element.active) {\r\n                            element.active = false;\r\n                        }\r\n                    }\r\n                }\r\n                else {\r\n                    if (element.transform.position.z - this.camaraPoint.transform.localPositionZ <= element[this.proUnitLen] * numShow) {\r\n                        const front = this.unitParent.getChildAt(element[this.proIndex] - 1);\r\n                        if (front && !front.active) {\r\n                            return;\r\n                        }\r\n                        if (!element.active) {\r\n                            element.active = true;\r\n                            if (index !== 0) {\r\n                                const pos = element.transform.position.clone();\r\n                                element.transform.position = new Laya.Vector3(pos.x, pos.y - 20, pos.z);\r\n                                LwgAni3D.moveTo(element, pos, 500, this, Laya.Ease.backInOut);\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        });\r\n    }\r\n}\r\n",

         GameData.Skin.setObjCompeletByID(this.openData.rewardId);\r\n                LwgDialogue.showTips('获得新皮肤！');\r\n                this.closeScene();\r\n            }\r\n            else {\r\n                LwgWX.AD.showVideo(() => {\r\n                    this.getRewardAni(3);\r\n                });\r\n            }\r\n        });\r\n    }\r\n    getRewardAni(multiple = 1) {\r\n        GameAni2D.getRewardAni(this.openData.rewardId, this.openData.rewardNum * multiple, () => {\r\n            this.owner.rewardNum.text = LwgTools.Format.strAddNum(this.owner.rewardNum.text, 20);\r\n        }, () => {\r\n            this.owner.rewardNum.text = '0';\r\n            this.closeScene();\r\n        });\r\n    }\r\n}\r\n",

  "code": "import { LwgClick, LwgCurrency, LwgDialogue, LwgScene, LwgWX } from \"../../Lwg/Lwg\";\r\nimport { GameData } from \"../Control/GameData\";\r\nimport { GameEnum } from \"../Control/GameEnum\";\r\nimport GameSceneName from \"../Control/GameSceneName\";\r\nimport { Control3D } from \"../Game3D/Control3D\";\r\nimport { SkinScene } from \"../Game3D/SkinScene\";\r\nexport default class Skin extends LwgScene.SceneBase {\r\n    constructor() {\r\n        super(...arguments);\r\n        this.arr1 = [];\r\n        this.arr2 = [];\r\n    }\r\n    lwgOnAwake() {\r\n        Control3D.cutSkinScene();\r\n        this.owner.list.renderHandler = new Laya.Handler(this, this.onListRender);\r\n        this.owner.list.hScrollBarSkin = '';\r\n        this.owner.list.array = GameData.Skin.arr;\r\n        for (let index = 0; index < GameData.Skin.arr.length; index++) {\r\n            const element = GameData.Skin.arr[index];\r\n            if (element.model === GameData.Skin.pitchName) {\r\n                this.dataPathWay = GameData.Role.getObjByID(element.ID);\r\n                this.updateBtn();\r\n            }\r\n        }\r\n    }\r\n    get curCurPrice() {\r\n        return this['_curCurPrice'] ? this['_curCurPrice'] : 1000;\r\n    }\r\n    set curCurPrice(val) {\r\n        this['_curCurPrice'] = val;\r\n        if (val) {\r\n            this.owner.price.text = val.toString();\r\n        }\r\n    }\r\n    onListRender(view, index) {\r\n        const data = view.dataSource;\r\n        view.skinIcon.skin = `Game/UI/SkinIcon/${data.model}.png`;\r\n        if (data.pitch) {\r\n            view.imgAniBoard.visible = true;\r\n        }\r\n        else {\r\n            view.imgAniBoard.visible = false;\r\n        }\r\n        if (data.complete) {\r\n            view.imgLock.visible = false;\r\n        }\r\n        else {\r\n            view.imgLock.visible = true;\r\n        }\r\n        LwgClick.off(undefined, view, this, null, null, this.onListRenderClick);\r\n        LwgClick.on(undefined, view, this, null, null, this.onListRenderClick, null, [[], [], [data, index], []]);\r\n    }\r\n    onListRenderClick(data, index0) {\r\n        SkinScene.ins.change(data.model);\r\n        for (let index = 0; index < this.owner.list.array.length; index++) {\r\n            const element = this.owner.list.array[index];\r\n            if (index0 === index) {\r\n                element.pitch = true;\r\n            }\r\n            else {\r\n                element.pitch = false;\r\n            }\r\n        }\r\n        if (data.complete) {\r\n            GameData.Skin.pitchName = data.model;\r\n        }\r\n        this.owner.list.refresh();\r\n        this.dataPathWay = GameData.Role.getObjByID(data.ID);\r\n        this.updateBtn();\r\n    }\r\n    updateBtn() {\r\n        if (this.dataPathWay.degreeNum === undefined) {\r\n            this.dataPathWay.degreeNum = 0;\r\n        }\r\n        let name;\r\n        switch (this.dataPathWay.pathWay) {\r\n            case GameEnum.RolePathWay.AD:\r\n                name = 'boxAD';\r\n                this.owner.boxADNum.text = '(' + this.dataPathWay.degreeNum + '/' + this.dataPathWay.condition + ')';\r\n                break;\r\n            case GameEnum.RolePathWay.CheckIn:\r\n                name = 'boxCheckIn';\r\n                break;\r\n            case GameEnum.RolePathWay.Free:\r\n                name = 'boxFree';\r\n                break;\r\n            case GameEnum.RolePathWay.Gold:\r\n                name = 'boxGold';\r\n                this.owner.boxGoldNum.text = this.dataPathWay.condition.toString();\r\n                break;\r\n            case GameEnum.RolePathWay.LotteryChest:\r\n                name = 'boxLotteryChest';\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n        if (this.dataPathWay.complete || this.dataPathWay.ID === 201) {\r\n            this.owner.btnGet.visible = false;\r\n        }\r\n        else {\r\n            this.owner.btnGet.visible = true;\r\n        }\r\n        for (let index = 0; index < this.owner.btnGet.numChildren; index++) {\r\n            const element = this.owner.btnGet.getChildAt(index);\r\n            if (element.name == name) {\r\n                element.visible = true;\r\n            }\r\n            else {\r\n                element.visible = false;\r\n            }\r\n        }\r\n    }\r\n    compelet() {\r\n        this.dataPathWay.complete = true;\r\n        GameData.Role.refreshAndStorage();\r\n        const obj = GameData.Skin.getObjByID(this.dataPathWay.ID);\r\n        obj.complete = true;\r\n        GameData.Skin.pitchName = obj.model;\r\n        GameData.Skin.refreshAndStorage();\r\n        this.owner.list.refresh();\r\n        this.updateBtn();\r\n    }\r\n    lwgButton() {\r\n        this.btnOnUp(this.owner.btnReturn, () => {\r\n            Control3D.cutMainScene();\r\n            this.openScene(GameSceneName.Start);\r\n        });\r\n        this.btnOnUp(this.owner.btnGet, () => {\r\n            if (this.dataPathWay.complete) {\r\n                LwgDialogue.showTips('已经获得！');\r\n                return;\r\n            }\r\n            if (this.dataPathWay.degreeNum === undefined) {\r\n                this.dataPathWay.degreeNum = 0;\r\n            }\r\n            switch (this.dataPathWay.pathWay) {\r\n                case GameEnum.RolePathWay.AD:\r\n                    LwgWX.AD.showVideo(() => {\r\n                        this.dataPathWay.degreeNum++;\r\n                        this.owner.boxADNum.text = '(' + this.dataPathWay.degreeNum + '/' + this.dataPathWay.condition + ')';\r\n                        if (this.dataPathWay.degreeNum >= this.dataPathWay.condition) {\r\n                            this.compelet();\r\n                        }\r\n                    });\r\n                    break;\r\n                case GameEnum.RolePathWay.CheckIn:\r\n                    LwgDialogue.showTips('签到页面获取！');\r\n                    break;\r\n                case GameEnum.RolePathWay.Free:\r\n                    LwgDialogue.showTips('已经获得！');\r\n                    break;\r\n                case GameEnum.RolePathWay.Gold:\r\n                    if (LwgCurrency.Diamond.num >= this.dataPathWay.condition) {\r\n                        this.compelet();\r\n                        LwgDialogue.showTips('购买成功！');\r\n                    }\r\n                    else {\r\n                        const data = {\r\n                            type: GameEnum.ResType.Diamond,\r\n                        };\r\n                        LwgScene.openOverlayScene(GameSceneName.ADGetReward, data);\r\n                    }\r\n                    break;\r\n                case GameEnum.RolePathWay.LotteryChest:\r\n                    LwgDialogue.showTips('宝箱页面获取！');\r\n                    break;\r\n                default:\r\n                    break;\r\n            }\r\n        });\r\n    }\r\n}\r\n",

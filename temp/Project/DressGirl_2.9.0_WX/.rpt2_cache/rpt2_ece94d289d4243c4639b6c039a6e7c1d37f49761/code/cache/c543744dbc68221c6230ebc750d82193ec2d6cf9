\r\n            this.boxArrowsR.visible = true;\r\n            this.aniPointingR.play(0, true);\r\n        }\r\n    }\r\n    step4() {\r\n        this.dynamicWord(5);\r\n        this.aniShowDialog.play(0, false);\r\n    }\r\n    dynamicWord(ID) {\r\n        this.lbDescribe.text = '';\r\n        const word = GameData.Guide.getValueByID(ID);\r\n        const time = word.length;\r\n        let index = 0;\r\n        LwgTimer.numLoop(50, time, this, () => {\r\n            this.lbDescribe.text += word.substr(index, 1);\r\n            index++;\r\n        });\r\n    }\r\n}\r\n",

tObj.name].instance.clone();\r\n                        this.unitParent.addChild(this.endUnit);\r\n                        this.endUnit.transform.position = new Laya.Vector3(0, 0, this.mileageTotal);\r\n                        this.changeFood();\r\n                        this.mileageTotal += unitObj.length;\r\n                        this.unitIndex++;\r\n                        console.log(unitObj.name);\r\n                    }\r\n                }\r\n            }\r\n        });\r\n    }\r\n    changeFood() {\r\n        for (let index = 0; index < this.endUnit.numChildren; index++) {\r\n            const child = this.endUnit.getChildAt(index);\r\n            if (child['newFood']) {\r\n                continue;\r\n            }\r\n            let newFood;\r\n            if (child.name.substr(0, 5) === 'FoodB' || child.name.substr(0, 5) === 'FoodA') {\r\n                newFood = GameRes.Prefab3D[this.lvFoodName].instance.clone();\r\n                this.endUnit.addChild(newFood);\r\n                newFood.transform.position = child.transform.position.clone();\r\n                newFood['newFood'] = true;\r\n                child.active = false;\r\n            }\r\n        }\r\n    }\r\n    unitHideAndShow() {\r\n        Laya.timer.frameLoop(1, this, () => {\r\n            for (let index = 0; index < this.unitParent.numChildren; index++) {\r\n                const unit = this.unitParent.getChildAt(index);\r\n                if (this.camaraPoint.transform.localPositionZ - unit.transform.position.z > 80) {\r\n                    unit.destroy(true);\r\n                }\r\n            }\r\n        });\r\n    }\r\n}\r\n",

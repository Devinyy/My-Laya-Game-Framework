;\r\n                LwgTimer.frameLoop(15, magma, () => {\r\n                    func();\r\n                }, true);\r\n                this.intoIndex++;\r\n            }\r\n            else {\r\n                Laya.timer.clearAll(magma);\r\n                this.magmaArr.splice(index0, 1);\r\n                this.outIndex++;\r\n            }\r\n            continueCb && continueCb();\r\n        }\r\n        clearAllMagma() {\r\n            for (let index = 0; index < this.magmaArr.length; index++) {\r\n                const magma = this.magmaArr[index];\r\n                Laya.timer.clearAll(magma);\r\n            }\r\n        }\r\n    }\r\n    StateControl.Magma = Magma;\r\n    class Destination {\r\n        constructor() {\r\n            Destination.ins = this;\r\n        }\r\n        action(continueCb) {\r\n            this.fCameraFP = Role.ins.cameraPoint.transform.position.clone();\r\n            const bossPoint = Role.ins.scene.getChildByName('unitParent').getChildByName('Road999').getChildByName('BossPoint');\r\n            this.boss = bossPoint.getChildAt(0);\r\n            console.log(this.boss);\r\n            const bossPos = this.boss.transform.position.clone();\r\n            bossPos.z -= 10;\r\n            const time = 1200;\r\n            LwgEvent.notify(GameEvent.hideRoleHP);\r\n            LwgAni3D.moveTo(Role.ins.cameraPoint, bossPos, time, this, null, () => {\r\n                const animator = this.boss.getComponent(Laya.Animator);\r\n                const state = GameData.Animator.getClipNameByRoleNameAndAni(this.boss.name, GameEnum.RoleAni.show);\r\n                animator.crossFade(state, 0.5);\r\n                Laya.timer.once(2500, this, () => {\r\n                    LwgAni3D.moveTo(Role.ins.cameraPoint, this.fCameraFP, 500, this, null, () => {\r\n                        const state1 = GameData.Animator.getClipNameByRoleNameAndAni(this.boss.name, GameEnum.RoleAni.idle);\r\n                        animator.crossFade(state1, 0.5);\r\n    